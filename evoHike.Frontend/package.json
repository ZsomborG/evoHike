{
  "name": "evohike-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,css,json}\" --log-level warn",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,css,json}\"",
    "type-check": "tsc --noEmit",
    "preview": "vite preview",
    "dev:backend": "dotnet run --launch-profile https --project ../evoHike.Backend/evoHike.Backend.csproj",
    "start:dev": "concurrently -n \"FRONTEND,BACKEND\" \"npm run dev\" \"npm run dev:backend\"",
    "postinstall": "cd .. && git rev-parse --is-inside-work-tree > NUL 2>&1 && git config core.hooksPath .husky",
    "prepare": "cd .. && husky"
  },
  "dependencies": {
    "@types/geojson": "^7946.0.16",
    "@types/leaflet": "^1.9.21",
    "axios": "^1.13.2",
    "eslint-plugin-react": "^7.37.5",
    "i18next": "^25.6.3",
    "i18next-browser-languagedetector": "^8.2.0",
    "leaflet": "^1.9.4",
    "leaflet-routing-machine": "^3.2.12",
    "react": "^19.1.1",
    "react-dom": "^19.2.0",
    "react-i18next": "^16.3.4",
    "react-icons": "^5.5.0",
    "react-leaflet": "^5.0.0",
    "react-leaflet-cluster": "^4.0.0",
    "react-router-dom": "^7.9.5",
    "react-spinners": "^0.17.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/eslint": "^9.6.1",
    "@types/node": "^24.6.0",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@types/react-i18next": "^7.8.3",
    "@vitejs/plugin-react-swc": "^4.1.0",
    "check-dependencies": "^2.0.0",
    "concurrently": "^9.2.1",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.6.2",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.45.0",
    "vite": "^7.1.7",
    "vite-plugin-checker": "^0.11.0"
  },
  "lint-staged": {
    "**/*.{ts,tsx,js,jsx,json,css}": [
      "prettier --write",
      "eslint --fix"
    ]
  }
}
